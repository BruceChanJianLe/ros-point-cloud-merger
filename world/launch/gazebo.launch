<launch>
    <arg name="world_name" value="$(find world)/worlds/I2R_LVL9.world"/>

    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <!-- colon-separated set of directories where Gazebo will search for models -->
        <env name="GAZEBO_MODEL_PATH" value="$(find world)/models"/>

        <!-- colon-separated set of directories where Gazebo will search for other resources such as world and media files. -->
        <!-- <env name="GAZEBO_RESOURCE_PATH" value="$(find world)/worlds:$(find world)"/> -->

        <!-- GAZEBO_MASTER_URI: URI of the Gazebo master. This specifies the IP and port where the server will be started and tells the clients where to connect to. -->
        <!-- GAZEBO_PLUGIN_PATH: colon-separated set of directories where Gazebo will search for the plugin shared libraries at runtime. -->
        <!-- GAZEBO_MODEL_DATABASE_URI: URI of the online model database where Gazebo will download models from. -->

        <arg name="world_name" value="$(arg world_name)"/>
        <arg name="paused" value="false"/>
        <arg name="use_sim_time" value="true"/>
        <arg name="gui" value="true"/>
        <arg name="recording" value="false"/>
        <arg name="debug" value="false"/>
    </include>
</launch>